<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.29.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1736772978274-1" author="Akira">
        <createTable tableName="credit_claim_outbox">
            <column name="id" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_credit_claim_outbox"/>
            </column>
            <column name="event_type" type="VARCHAR(255)"/>
            <column name="claim_id" type="UUID"/>
            <column name="event_payload" type="TEXT">
            </column>
            <column name="created_at" type="DATETIME">
                <constraints nullable="false"/>
            </column>
            <column name="processing_status" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>